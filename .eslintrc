{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": [2, "never"],
    "quotes": [2, "single"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-reserved-keys": 0
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEV__": true
  }
}
