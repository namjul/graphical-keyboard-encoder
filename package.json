{
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s",
    "build": "webpack",
    "build:dist": "NODE_ENV=production npm run build",
    "prebuild:watch": "npm run clean -s",
    "build:watch": "webpack -w",
    "serve": "webpack-dev-server --inline --hot --progress --colors --content-base ./src",
    "start": "node src/server.js",
    "todo": "grep --color -Ion '\\(TODO\\).*' -r src || true",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "express": "^4.13.3",
    "http-proxy": "^1.11.2",
    "react": "^0.14.8",
    "react-addons-update": "^0.14.0",
    "react-dom": "^0.14.8"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.12.0",
    "ncp": "^2.0.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
